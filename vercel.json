{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "urlencoderdecoder.com"
        }
      ],
      "destination": "https://www.urlencoderdecoder.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(http|https)://urlencoderdecoder.com"
        }
      ],
      "destination": "https://www.urlencoderdecoder.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.urlencoderdecoder.com"
        },
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://www.urlencoderdecoder.com/:path*",
      "permanent": true
    }
  ]
}
